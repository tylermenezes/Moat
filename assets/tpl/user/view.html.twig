{% extends "template.html.twig" %}
{% block content %}
    <section class="user content">
        <header>
            <h1>{{ user.name }}</h1>
        </header>
        <img src="{{ user.picture }}" />
        <p>Email: <a href="mailto:{{ user.email }}">{{ user.email }}</a></p>
        <p>Phone: {{ user.phone }}</p>
    </section>
{% endblock %}
